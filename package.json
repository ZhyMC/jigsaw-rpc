{
  "name": "jigsaw-rpc",
  "version": "0.2.0",
  "description": "jigsaw-rpc is a RPC framework under Node.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "mocha",
    "build": "node ./scripts/build",
    "prepare": "npm run compile",
    "test-cov": "nyc --reporter=lcov npm test",
    "dev": "nodemon --watch ./src/ --watch ./examples/ -e ts --exec \"ts-node ./examples/test-app.ts\" ",
    "compile": "tsc",
    "start": "node ./bin/cli",
    "deploy": "node ./scripts/deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZhyMC/jigsaw-rpc.git"
  },
  "keywords": [
    "rpc",
    "http",
    "client",
    "tcp",
    "udp",
    "remote",
    "call",
    "framework",
    "json-rpc",
    "jsonrpc",
    "jsonrpc2",
    "web"
  ],
  "author": "zhymc",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/ZhyMC/jigsaw-rpc/issues"
  },
  "homepage": "https://github.com/ZhyMC/jigsaw-rpc#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.25",
    "cz-conventional-changelog": "^3.3.0",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.5",
    "debug": "^4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "org": "wyattzhy",
    "expose_port": [
      "3793"
    ]
  },
  "dependencies": {
    "debug": "^4.2.0",
    "tiny-typed-emitter": "^2.0.3",
    "config-style-cli": "^0.0.2",
    "meow": "^9.0.0"
  }
}
